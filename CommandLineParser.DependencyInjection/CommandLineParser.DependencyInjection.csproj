<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>Microsoft Dependency Injection extensions for CommandLineParser (https://github.com/commandlineparser/commandline)</Description>
    <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
    <Product>CommandLineParser.DependencyInjection</Product>
    <PackageId>CommandLineParser.DependencyInjection</PackageId>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <Version>1.0.0</Version>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <Company>CommandLineParser.DependencyInjection</Company>
    <Authors>CommandLineParser.DependencyInjection</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.6.0" />
    <PackageReference Include="Scrutor" Version="3.1.0" />
  </ItemGroup>

</Project>
